<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ensardz.yupivoyenrique">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MainPinkTheme">

        <activity android:name=".Activities.SplashActivity" android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activities.MainActivity"
            android:configChanges="orientation|screenSize"
            android:theme="@style/MainPinkTheme.NoActionBar"/>
        <activity
            android:name=".Activities.QuienesSomosActivity"
            android:label="@string/quienes_somos_titulo"
            android:parentActivityName=".Activities.MainActivity"
            android:theme="@style/SecondaryPurpleTheme" />
        <activity
            android:name=".Activities.PorqueElegirActivity"
            android:label="@string/porque_elegir_titulo"
            android:parentActivityName=".Activities.MainActivity"
            android:theme="@style/SecondaryPurpleTheme" />
        <activity
            android:name=".Activities.ContactoActivity"
            android:label="@string/contacto_titulo"
            android:parentActivityName=".Activities.MainActivity"
            android:theme="@style/SecondaryPurpleTheme" />
        <activity
            android:name=".Activities.FormulariosContainerActivity"
            android:parentActivityName=".Activities.MainActivity" />
        <activity android:name=".Activities.LoginActivity">
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_application_id" />

    </application>

</manifest>